{
  "env" : {
    "node" : true
  },
  "rules" : {
    "semi": [2, "never"],
    "strict": false,
    "quotes": [1, "double", "avoid-escape"],
    "no-use-before-define": [2, "nofunc"],
    "curly": false,
    "no-underscore-dangle": 0,
    "no-lonely-if": 1,
    "no-unused-vars": [2, {"vars" : "all", "args" : "after-used"}],
    "no-mixed-requires": 0,
    "space-infix-ops": 0
  }
}
